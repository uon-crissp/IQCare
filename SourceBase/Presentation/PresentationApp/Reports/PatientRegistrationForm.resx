  <data name="PatientInfo.SelectCommand" xml:space="preserve">
    <value>exec('Open symmetric key Key_CTC decryption by password=''ttwbvXWpqb5WOLfLrBgisw==''' )
                              
SELECT top 1 (select top 1 FacilityName from mst_Facility where DeleteFlag=0) as FacilityName
	,convert(VARCHAR(50), decryptbykey(a.FirstName)) AS [FirstName]
	,convert(VARCHAR(50), decryptbykey(a.MiddleName)) AS [MiddleName]
	,convert(VARCHAR(50), decryptbykey(a.LastName)) AS [LastName]
	,Coalesce(a.PatientEnrollmentID, a.PatientClinicID, a.patientIPNo) as PatientID
	,a.[ID/PassportNo] as IDNumber
	,a.RegistrationDate as RegistrationDate
	,case when a.Sex=16 then 'Male' else 'Female' end as Gender
	,a.DOB
	,Convert(VARCHAR, datediff(month, a.DOB, getdate()) / 12) [Age]
	,a.Landmark
	,a.LocalCouncil as Residence
	,(select top 1 x.Name from mst_Village x where x.id=a.VillageName) as village
	,(select top 1 x.Name from mst_District x where x.id=a.DistrictName) as subcounty
	,(select top 1 x.Name from mst_Province x where x.id=a.Province) as County
	,convert(VARCHAR(50), decryptbykey(a.Phone)) AS [Phone]
	,(select top 1 x.Name from mst_Decode x where x.id=a.MaritalStatus) as maritalStatus
	,b.EmergContactName
	,(select top 1 x.Name from mst_RelationshipType x where x.id=b.EmergContactRelation) as EmergContactRelation
	,b.EmergContactPhone
	,b.EmergContactAddress
	,b.NextofKinName
	,(select top 1 x.Name from mst_RelationshipType x where x.id=b.NextofKinRelationship) as NextofKinRelationship
	,b.NextofKinTelNo
	,b.NextofAddress
FROM mst_patient a
inner join [DTL_PATIENTCONTACTS] b on a.Ptn_Pk=b.ptn_pk
WHERE a.ptn_pk = @patientpk
order by b.VisitId desc

CLOSE symmetric KEY Key_CTC</value>
